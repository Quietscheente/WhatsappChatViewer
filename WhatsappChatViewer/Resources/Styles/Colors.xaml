<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Color x:Key="Primary">#504137</Color>
    <Color x:Key="Secondary">#1B998B</Color>
    <Color x:Key="Tertiary">#F6AE2D</Color>
    <Color x:Key="BackgroundPrimary">#DBE9EE</Color>
    <Color x:Key="BackgroundSecondary">#D0F1BF</Color>

    <Color x:Key="Textblock">#444444</Color>
    <Color x:Key="Error">#E9806E</Color>
    <Color x:Key="Hyperlink">#2B0B98</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}"/>

</ResourceDictionary>